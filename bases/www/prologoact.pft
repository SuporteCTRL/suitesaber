'<script>

function FormatearSubcamposRepetibles(campo,sep_sc,sep_occ){

     resultado="" 
     occ=campo.split("$$$")
     limite=occ.length

     for (ix_occ=0;ix_occ<limite;ix_occ++){
         campo=occ[ix_occ]
         c=campo.split("^")
         total=c.length-1
         for (ix=0;ix<=total;ix++){
             if (c[ix]!=""){
                 resultado+=c[ix].substr(1)
                 if (ix!=total)
                     resultado+=sep_sc
             }
         }
         resultado+=sep_occ
     }
	return resultado

}
</script>

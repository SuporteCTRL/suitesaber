[FILE_LOCATION]

FILE DATABASE.*=/var/opt/ABCD/bases/nonunicode/data/nonunicode.*
FILE standard.pft=/var/opt/ABCD/bases/nonunicode/pfts/%lang%/nonunicode.pft
FILE SHORTCUT.IAH=/var/opt/ABCD/bases/nonunicode/pfts/%lang%/shortcut.pft
FILE GXML=%path_database%gizmo/gXML.*

[INDEX_DEFINITION]

INDEX AL=^1palabras^2palabras^3all words^4mots^d*^xAL ^yDATABASE^uAL_^mAL_
INDEX SA=^1Devanagari^2Devanagari^3Devanagari^4Devanagari^xSA ^yDATABASE^uSA_^mSA_
INDEX AM=^1Amharic^2Amharic^3Amharic^4Amharic^xAM ^yDATABASE^uAM_^mAM_

[APPLY_GIZMO]


[FORMAT_NAME]

FORMAT standard.pft=^1normal^2normal^3default^4normal
FORMAT DEFAULT=standard.pft

[HELP_FORM]


[PREFERENCES]

AVAILABLE FORMS=
SEND RESULT BY EMAIL=OFF
NAVIGATION BAR=OFF
DOCUMENTS PER PAGE=20
FEATURES=
